        <jsScript_script>//Script here
var hora2 = hora[0] + hora[1] + ":" + hora[2] + hora[3];

var dia_temp = "";
if (dia.length == 1) {
	dia_temp = "0" + dia;
} else {
	dia_temp = dia;
}

var mes_temp = "";
if (mes.length == 1) {
	mes_temp = "0" + mes;
} else {
	mes_temp = mes;
}

var data = ano + "-" + mes_temp + "-" + dia_temp + " " + hora2 + ":00";</jsScript_script>
      </jsScript>
    </jsScripts>
