    <formula>
      <field_name>part_name</field_name>
      <formula_string>SUBSTITUTE(
MID([short_filename]; 1; INT(
FIND("2"; [short_filename])
) - 2); "_"; " ")</formula_string>
      <value_type>String</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
      <replace_field/>
    </formula>
    <formula>
      <field_name>part_name2</field_name>
      <formula_string>MID([short_filename]; INT(
FIND("2"; [short_filename])
); LEN([short_filename]))</formula_string>
      <value_type>String</value_type>
      <value_length>-1</value_length>
      <value_precision>-1</value_precision>
